
public class DeclaracionesOpeadores {

	public static void main(String[] args) {
		
			int a=5; // double es para numero con coma
			int b;
			b=7;
			int c=b+a;
			
			final double pulgadas=2.54; // final es para constantes (no se pueden modificar posteriormente)
			double cm=6;
			double resultado=cm/pulgadas;
			System.out.println(c);
			System.out.println("en " +cm +"cm hay"+ resultado +"pulgadas");
	}

}
